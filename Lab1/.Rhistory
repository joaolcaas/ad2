library(ggplot2)
library(dplyr)
df<- read.csv("/home/joaolaf/Área de Trabalho/AD2/Lab1/dadosCEAP.csv")
a <- df %>% group_by(sgPartido) %>%
summarise(qntPartido = n(), totalgasto= sum(valorLíquido))
b<- df %>% group_by(tipoDespesa)%>%
summarise(quantasDespesas=n(),totalgasto = sum(valorLíquido))
a<-a[!is.na(a$sgPartido),]
a %>%  ggplot(aes(x = reorder(sgPartido,-qntPartido),qntPartido), y =qntPartido) +
geom_bar(stat = "identity") +
theme(axis.text.x = element_text(angle = 45, hjust = 1),
panel.background = element_blank()) + xlab("Sigla do Partido") + ylab("Quantidade de vezes que gastou")
a %>%  ggplot(aes(x = reorder(sgPartido,-totalgasto),totalgasto), y = options()$totalgasto) +
geom_bar(stat = "identity") +
theme(axis.text.x = element_text(angle = 45, hjust = 1),
panel.background = element_blank()) + xlab("Sigla do Partido") + ylab("Mais uso em R$")
b %>%  ggplot(aes(x = reorder(tipoDespesa,-quantasDespesas),quantasDespesas), y = options()$quantasDespesas) +
geom_bar(stat = "identity") +
theme(axis.text.x = element_text(angle = 45, hjust = 1),
panel.background = element_blank()) + xlab("Sigla do Partido") + ylab("Quantidade de vezes que gastou")
b1<- b [order(b$quantasDespesas,decreasing = TRUE),]
b3<-head(b1,3)
b5<- tail(b1,3)
despesas_frame<- rbind(b3,b5)
despesas_frame %>%  ggplot(aes(x = reorder(tipoDespesa,-quantasDespesas),quantasDespesas), y = options()$quantasDespesas) +
geom_bar(stat = "identity",fill="#FF6666") +
theme(axis.text.x = element_text(angle = 45, hjust = 1),
panel.background = element_blank()) + xlab("Sigla do Partido") + ylab("Quantidade de vezes que gastou")
b2<-b[order(b$totalgasto,decreasing = TRUE),]
b4<-head(b2,3)
b6<-tail(b2,3)
despesas_frame_gastos<- rbind(b4,b6)
despesas_frame_gastos %>%  ggplot(aes(x = reorder(tipoDespesa,-totalgasto),totalgasto), y = options()$totalgasto) +
geom_bar(stat = "identity",fill="#FF1111")+
theme(axis.text.x = element_text(angle = 45, hjust = 1),
panel.background = element_blank()) + xlab("Sigla do Partido") + ylab("Gasto por R$")
c<- df %>% group_by(nomeParlamentar,sgUF,sgPartido)%>%
summarise()
c1<- c %>% group_by(sgUF)%>%
summarise(totalUF=n())
c1<-c1[!is.na(c1$sgUF),]
c1<-c1[order(c1$totalUF,decreasing = TRUE),]
c1%>% ggplot(aes(x=reorder(sgUF,-totalUF),totalUF),y=totalUF) + geom_bar(width = 0.5,stat="identity") + labs(x="Estados",y="Total de partidos(candidato eleito)")
cc <- df %>% group_by(sgUF,sgPartido,valorLíquido) %>%
summarise()
cc1 <- cc%>%group_by(sgUF)%>%
summarise(valorPorEstado = sum(valorLíquido))
cc1 <- cc1[!is.na(cc1$sgUF),]
cc1 %>% ggplot(aes(x= reorder(sgUF,-valorPorEstado),valorPorEstado),y= options()$valorPorEstado) + geom_bar(width = 0.5,stat = "identity") + labs(x="Estados",y="Valor gasto por estado presente no CEAP",title="Maiores consumidores CEAP por estado")
estado <- df%>%group_by(nomeParlamentar,sgUF) %>%
summarise(quantasVezesEstado = n())
estado2 <-df %>%group_by(nomeParlamentar,tipoDespesa)%>%
summarise(quantasVezesDespesa=n())
estado <- estado[order(-estado$quantasVezesEstado),]
estado1 <- head(estado,2)
estado1 %>% ggplot(aes(x=reorder(sgUF,-quantasVezesEstado),quantasVezesEstado),y=quantasVezesEstado) + geom_bar(stat="identity") + labs(x="Estados",y="Numero de requisições")
SPgasto <- df%>% group_by(nomeParlamentar,valorLíquido) %>% summarise(valorParaPolitico = sum(valorLíquido),sgUF="SP")
SPgasto <- SPgasto[order(-SPgasto$valorParaPolitico),]
SPgasto <- head(SPgasto,2)
SPgasto %>% ggplot(aes(x=reorder(nomeParlamentar,-valorParaPolitico),valorParaPolitico),y=options()$valorParaPolitico) + geom_bar(stat = "identity")
d<- df %>% group_by(nomeParlamentar) %>%
summarise(quantasVezesGasta = n(),totalgastoParla = sum(valorLíquido))
d<-d[order(-d$totalgastoParla),]
d1<-head(d,2)
d2<-tail(d,2)
d1 %>% ggplot( aes(y = totalgastoParla, x = nomeParlamentar)) +
geom_boxplot(na.rm = TRUE)
labs(title = "Notas dos alunos da UFCG por curso", x = "Código do curso", y = "Notas") +
theme(axis.text.x = element_text(angle = 90, hjust = 1))
